{
  "name": "Spry+Nimble",
  "version": "3.4.0",
  "summary": "Nimble matcher for test expectations on Spyable objects.",
  "description": "Spry+Nimble allows developers, used to working with Quick/Nimble, be able to test whether or not functions were called on objects in a helpful Nimble matcher. The haveReceived matcher contains rich failure messages when tests fail.",
  "source": {
    "git": "git@github.com:NikSativa/NCallback.git"
  },
  "homepage": "https://github.com/NikSativa/NCallback",
  "license": "MIT",
  "authors": {
    "Nikita Konopelko": "nik.sativa@gmail.com"
  },
  "social_media_url": "https://www.facebook.com/Nik.Sativa",
  "swift_versions": "5.0",
  "platforms": {
    "ios": "10.0"
  },
  "source_files": "SourceNimble/*",
  "frameworks": "XCTest",
  "dependencies": {
    "Nimble": [
      "~> 9.0.0"
    ],
    "Spry": [
      "~> 3.4.0"
    ]
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "dependencies": {
        "Quick": [
          "~> 3.1.2"
        ]
      },
      "source_files": "Tests/Specs/**/*.swift",
      "resources": [
        "Tests/Specs/**/*.{storyboard,xib,xcassets,json,imageset,png,strings,stringsdict}"
      ]
    }
  ],
  "swift_version": "5.0"
}
